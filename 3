from flask import Flask, render_template
import sqlite3
app = Flask(__name__)

@app.route('/')
def index():
    return render_template('index.html')

@app.route('/a/<articleID>')
def article(articleID):
    conn = sqlite3.connect('/static/articles.db')
    cursor = conn.execute('select body from articles')
    for row in cursor:
        print row[0]
    conn.close


